<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <Color x:Key="AppBackground">#33001a</Color>
    <Color x:Key="ButtonColor">#940034</Color>
    <Color x:Key="SelectedColor">#e60050</Color>
    <Color x:Key="SpecialColor">#bd3061</Color>
    <Color x:Key="NavBarColor">#640032</Color>
    <Color x:Key="InactiveColor">#784f5d</Color>

    <Color x:Key="White">#FFFFFF</Color>

    <SolidColorBrush x:Key="AppBackgroundBrush" Color="{StaticResource AppBackground}" />
    <SolidColorBrush x:Key="ButtonBrush" Color="{StaticResource ButtonColor}" />
    <SolidColorBrush x:Key="SelectedBrush" Color="{StaticResource SelectedColor}" />
    <SolidColorBrush x:Key="NavBarBrush" Color="{StaticResource NavBarColor}" />
    <SolidColorBrush x:Key="SpecialBrush" Color="{StaticResource SpecialColor}" />
    <SolidColorBrush x:Key="WhiteBrush" Color="{StaticResource White}" />
    <SolidColorBrush x:Key="InactiveBrush" Color="{StaticResource InactiveColor}" />

</ResourceDictionary>
